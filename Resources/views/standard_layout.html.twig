{% extends 'SonataAdminBundle::standard_layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('bundles/vincecmssonataadmin/css/main.css') }}" type="text/css" rel="stylesheet" />
    {% if form is defined %}
        {{ form_stylesheets(form) }}
    {% endif %}
{% endblock %}

{% block footer %}
    {{ parent() }}
    {% if form is defined %}
        {% javascripts '@VinceCmsSonataAdminBundle/Resources/public/js/url.js' filter='?yui_js' %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {{ form_javascripts(form) }}
    {% endif %}
{% endblock %}